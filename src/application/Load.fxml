<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane stylesheets="@menu.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutX="-8.0">
         <image>
            <Image url="@Sprites/load.png" />
         </image>
      </ImageView>
      <ImageView id="load" fx:id="loadButton" fitHeight="37.0" fitWidth="156.0" layoutX="356.0" layoutY="292.0" onMouseClicked="#loadMap" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Sprites/load_button.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <ImageView fx:id="mapButton" fitHeight="150.0" fitWidth="200.0" layoutX="359.0" layoutY="125.0" onMouseClicked="#presetMap" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Sprites/preset_map1.JPG" />
         </image>
      </ImageView>
      <Text fill="#e8cfcf" layoutX="342.0" layoutY="104.0" strokeType="OUTSIDE" strokeWidth="0.0" text="select preset map or load by file name">
         <font>
            <Font name="Calibri Light" size="12.0" />
         </font>
      </Text>
